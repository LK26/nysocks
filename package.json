{
  "name": "kcpuv",
  "version": "1.0.0",
  "description": "## Build",
  "main": "node_lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "dev": "babel node/src --out-dir node/lib -w",
    "build": "babel node/src --out-dir node/lib",
    "install": "node-gyp rebuild",
    "test:js": "jest",
    "test:packet": "node node/lib/__tests__/send_a_buf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://OYYD@bitbucket.org/OYYD/kcpuv.git"
  },
  "author": "",
  "license": "ISC",
  "gypfile": true,
  "homepage": "https://bitbucket.org/OYYD/kcpuv#readme",
  "dependencies": {
    "nan": "^2.7.0",
    "socksv5": "^0.0.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^21.2.1"
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "jest": true
    },
    "extends": "airbnb-base",
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "import/prefer-default-export": 0
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "target": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "jest": {
    "testMatch": ["**/node/src/**/__tests__/**/*.spec.js"]
  }
}
